{
    "restartable": "rs",
    "ignore": [".git", "node_modules/", "dist/", "coverage/"],
    "execMap": {
        "ts,tsx,": "webpack --watch --liveReload --host 127.0.0.1 --port 9000 --color --progress",
        "js,jsx,html": "webpack --watch --liveReload --host 127.0.0.1 --port 9000 --color --progress"
    },
    "watch": ["src", "dist"],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js,json,ts,tsx,vue,html,jsx,css,scss"
}
