{
    "name": "svelte-app",
    "version": "1.0.0",
    "main": "src/app.js",
    "scripts": {
        "prettier-watch": "npx onchange \"**/*\" -- npx prettier --write --ignore-unknown {{changed}}",
        "prettier": "prettier --write --plugin-nsearch-dir=. ./**",
        "nodemon": "nodemon",
        "test": "jest --watchAll",
        "start": "webpack ./src/app.js --mode development --watch",
        "build-css": "tailwindcss build tailwindcss-tutorial/src/styles.css -o tailwindcss-tutorial/public/styles.css",
        "build": "webpack",
        "serve": "webpack serve",
        "lint": "eslint ./src",
        "lint:fix": "./node_modules/.bin/eslint --ignore-path .gitignore --fix --color"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.0",
        "@babel/core": "^7.13.8",
        "@babel/preset-env": "^7.13.9",
        "@typescript-eslint/eslint-plugin": "^4.16.1",
        "@typescript-eslint/parser": "^4.16.1",
        "babel": "^6.23.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "babel-preset-env": "^1.7.0",
        "css-loader": "^5.1.1",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-html": "^6.1.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.5",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "husky": "^5.1.3",
        "jasmine": "^3.6.4",
        "jest": "^26.6.3",
        "jsdoc": "^3.6.6",
        "lint-staged": "^10.5.4",
        "parcel": "^1.12.4",
        "puppeteer": "^8.0.0",
        "rollup": "^2.40.0",
        "rollup-plugin-babel": "^4.4.0",
        "sirv-cli": "^1.0.0",
        "snowpack": "^3.0.13",
        "style-loader": "^2.0.0",
        "ts-loader": "^8.0.17",
        "typescript": "^4.2.2",
        "webpack": "^5.24.2",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        ".js*": [
            "./node_modules/.bin/eslint --ignore-path .gitignore --fix --color",
            "git add"
        ],
        ".ts*": [
            "./node_modules/.bin/eslint --ignore-path .gitignore --fix --color",
            "git add"
        ]
    },
    "dependencies": {
        "axios": "^0.21.1",
        "ejs": "^3.1.6",
        "tailwindcss": "^2.0.3"
    }
}
